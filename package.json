{
  "name": "TODO",
  "description": "TODO",
  "version": "0.0.1",
  "scripts": {
    "commit": "npx git-cz",
    "dev": "sapper dev",
    "build": "sapper build",
    "export": "sapper export",
    "format": "prettier --write \"**/*.{js,json,mjs,ts,svelte}\" --ignore-path .gitignore",
    "postformat": "npm run lint:fix",
    "lint:fix": "eslint --fix --ignore-path .gitignore --ignore-pattern '**/*.d.ts' '**/*.{js,ts,svelte}'",
    "lint-staged": "lint-staged",
    "prestart": "npm run build",
    "start": "node __sapper__/build",
    "cy:run": "cypress run",
    "cy:open": "cypress open",
    "test": "run-p --race dev cy:run"
  },
  "dependencies": {
    "compression": "^1.7.1",
    "express": "^4.17.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^8.1.0",
    "@commitlint/config-conventional": "^8.1.0",
    "@commitlint/prompt": "^8.1.0",
    "@types/compression": "^1.0.1",
    "@types/express": "^4.17.1",
    "@types/node": "^12.7.3",
    "babel-eslint": "^10.0.3",
    "bulma": "^0.7.5",
    "commitizen": "^4.0.3",
    "eslint": "^6.3.0",
    "eslint-config-prettier": "^6.1.0",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-svelte3": "^2.7.3",
    "husky": "^3.0.5",
    "lint-staged": "^9.2.5",
    "node-sass": "^4.12.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^7.0.17",
    "prettier": "^1.18.2",
    "prettier-plugin-svelte": "^0.7.0",
    "pretty-quick": "^1.11.1",
    "sapper": "^0.27.0",
    "standard-version": "^7.0.0",
    "svelte": "^3.9.1",
    "svelte-loader": "^2.9.0",
    "svelte-preprocess": "^3.0.2",
    "ts-loader": "^6.0.4",
    "types-serviceworker": "0.0.1",
    "typescript": "^3.6.2",
    "webpack": "^4.39.3"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "config": {
    "commitizen": {
      "path": "@commitlint/prompt"
    }
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -e $HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.ts": [
      "npm run format",
      "git add"
    ]
  }
}
